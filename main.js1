var num = 10;

// add 10 to the number
num = num + 10;
console.log("Add 10:", num);

// subtract 5 from the number
num = num - 5;
console.log("Subtract 5:", num);

// multiply the number by 7
num = num * 7;
console.log("Multiply by 7:", num);

// divide the number by 5
num = num / 5;
console.log("Divide by 5:", num);
